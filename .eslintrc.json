{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "import/order": [
      "warn",
      {
        "alphabetize": { "order": "asc", "caseInsensitive": true },
        "groups": [
          "builtin",
          "external",
          "internal",
          ["parent", "sibling", "index"],
          "object",
          "type"
        ],
        "newlines-between": "always"
      }
    ]
  }
}
